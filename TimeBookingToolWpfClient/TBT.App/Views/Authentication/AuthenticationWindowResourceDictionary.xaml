<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:TBT.App.Views.Authentication"
                    xmlns:vm="clr-namespace:TBT.App.ViewModels.Authentication">
    <DataTemplate DataType="{x:Type vm:AuthenticationControlViewModel}">
        <local:AuthenticationControl/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:ForgotPasswordControlViewModel}">
        <local:ForgotPasswordControl/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:ResetPasswordControlViewModel}">
        <local:ResetPasswordControl/>
    </DataTemplate>
</ResourceDictionary>