                    xmlns:vm="clr-namespace:TBT.App.ViewModels.MainWindow">
    <DataTemplate DataType="{x:Type vm:CalendarTabViewModel}">
        <local:CalendarControl/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:EditTimeEntryViewModel}">
        <local:EditTimeEntryControl/>
