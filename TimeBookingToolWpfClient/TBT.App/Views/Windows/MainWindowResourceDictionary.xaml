    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:ProjectsTabViewModel}">
        <local:SpinnersApple/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:TasksTabViewModel}">
